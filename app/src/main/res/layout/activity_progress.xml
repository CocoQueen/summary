<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tc="http://schemas.android.com/apk/res-auto"
    tools:context="com.coco.zxingdemo.progress.ProgressActivity">


    <com.coco.zxingdemo.progress.CustomCircleProgress
        android:id="@+id/mProgresss"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tc:circleColor="@color/white"
        tc:radius="50dip"
        tc:ringBgColor="@color/white2"
        tc:ringColor="@color/colorRed"
        tc:strokeWidth="10dip"
    />
</RelativeLayout>
